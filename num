import java.util.Scanner;
class number
{
    public static void main(String args[])
    {
        int num;
        Scanner scanner=new Scanner(System.in);

        num=scanner.nextInt();
        if (num%2==0)
        {
           System.out.println("NUMBER IS EVEN");
        }
        else
        {
         System.out.println("NUMBER IS ODD");
        }
        
    }
}